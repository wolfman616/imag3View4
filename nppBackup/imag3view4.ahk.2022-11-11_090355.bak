	DllCall("gdi32.dll\SetStretchBltMode","Uint",mDC,"int",1)
	DllCall("gdi32.dll\StretchBlt","UInt",mDC00,"Int",0,"Int",0,"Int"
	,CURRENT_W,"Int",CURRENT_h,"UInt",mDC%oio%, "UInt",0
