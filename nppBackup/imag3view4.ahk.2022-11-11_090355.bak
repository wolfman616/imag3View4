		DllCall( "GetCursorPos","Uint",&RECT)
			sleep,4
		win_move(hgui,(NumGet(&RECT,0,"Int") - xs),(NumGet(&RECT,4,"Int") - ys),CURRENT_W,CURRENT_H,0x4001) ; DllCall("MoveWindow",  "Uint",hwnd1,"int",vWinX,"int",vWiny,"int",rw,"int",rh,"Int",2) #
