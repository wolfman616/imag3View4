			tt(pic_arr[oio].xtn " file.")
	} ;	tt(oio "`n" pic_arr[oio].CurrPath  "`n" pic_arr[oio].xtn)
	Xcrete(oio)
	Xcrete(oio,"upd8")
	(!(iold2 = oio)?Xcrete(iOLD2,"old_delete"))
	return,
}

